<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <Class name="rife.bld.extension.tools.ClasspathUtils"/>
        <Method name="joinClasspathJar"/>
        <Bug pattern="OCP_OVERLY_CONCRETE_COLLECTION_PARAMETER"/>
    </Match>
    <Match>
        <Or>
            <Class name="rife.bld.extension.tools.IOUtils"/>
            <Method name="notExists" param="java.lang.String"/>
            <Method name="resolveFile"/>
        </Or>
        <Bug pattern="PATH_TRAVERSAL_IN"/>
    </Match>
</FindBugsFilter>